<div class="user">
    <h2>哪些产品在使用</h2>
    <div class="user-list">
        <% item.forEach(function(data) { %>
            <div class="item">
                <div class="icon">
                    <img src="<%= data.img %>" />
                </div>
            </div>
        <% }) %>
    </div>    
</div>